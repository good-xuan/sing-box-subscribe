{
  "dns": {
    "servers": [
      {
        "tag": "dns_cf",
        "address": "h3://dns.cloudflare.com/dns-query",
        "address_resolver": "resolver",
        "strategy": "ipv4_only",
        "detour": "manual_out"
      },
      {
        "tag": "dns_ali",
        "address": "h3://dns.alidns.com/dns-query",
        "address_resolver": "resolver",
        "strategy": "ipv4_only",
        "detour": "direct_out"
      },
      {
        "tag": "resolver",
        "address": "223.5.5.5",
        "strategy": "ipv4_only",
        "detour": "direct_out"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "dns_ali"
      },
      {
        "clash_mode": "direct",
        "server": "dns_ali"
      },
      {
        "clash_mode": "global",
        "server": "dns_cf"
      },
      {
        "rule_set": "geosite-geolocation-cn",
        "server": "dns_ali"
      }
    ],
    "final": "dns_cf"
  },
 "ntp": {
    "enabled": true,
    "server": "time.apple.com",
    "server_port": 123,
    "interval": "30m0s",
    "detour": "direct_out"
  },
 "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "tun0",
      "inet4_address": "192.168.255.1/30",
      "auto_route": true,
      "stack": "mixed",
      "sniff": true
    },
    {
      "type": "http",
      "tag": "http_in",
      "listen": "0.0.0.0",
      "listen_port": 20171
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "manual_out",
      "outbounds": [
        "auto_out",
        "all"
      ],
    },
    {
      "type": "urltest",
      "tag": "auto_out",
      "outbounds": [
       "all"
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "10m",
      "interrupt_exist_connections": true
    },
    {
      "type": "direct",
      "tag": "direct_out"
    },
    {
      "type": "block",
      "tag": "block_out"
    },
    {
      "type": "dns",
      "tag": "dns_out"
    }
  ],
  "route": {
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns_out"
      },
      {
        "ip_is_private": true,
        "outbound": "direct_out"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct_out"
      },
      {
        "clash_mode": "global",
        "outbound": "manual_out"
      },
      {
        "rule_set": [
          "geosite-category-ads-all"
        ],
        "outbound": "block_out"
      },
      {
        "rule_set": [
          "geoip-cn",
          "geosite-geolocation-cn"
        ],
        "outbound": "direct_out"
      },
	  {
        "inbound": "http_in",
        "outbound": "manual_out"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "geoip-cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs",
        "download_detour": "manual_out",
        "update_interval": "7d"
      },
      {
        "type": "remote",
        "tag": "geosite-geolocation-cn",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-cn.srs",
        "download_detour": "manual_out",
        "update_interval": "7d"
      },
      {
        "type": "remote",
        "tag": "geosite-category-ads-all",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-category-ads-all.srs",
        "download_detour": "manual_out",
        "update_interval": "7d"
      }
    ],
    "final": "manual_out",
    "auto_detect_interface": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true
    },
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "ui",
      "external_ui_download_url": "https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "manual_out",
      "default_mode": "rule"
    }
  }
}
